Checks: '-*,google-*'
WarningsAsErrors: '*'
HeaderFilterRegex: '.*'
AnalyzeTemporaryDtors: false
CheckOptions:
  - key: google-readability-braces-around-statements.ShortStatementLines
    value: '0'
  - key: google-readability-function-size.StatementThreshold
    value: '800'
  - key: google-readability-namespace-comments.ShortNamespaceLines
    value: '10'
  - key: google-readability-todo.TPattern
    value: '(TODO|todo)'
  - key: google-runtime-int.IntTypesMinCheck
    value: '64'
  - key: google-runtime-int.IntTypes
